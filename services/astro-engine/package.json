{
    "name": "@grahvani/astro-engine",
    "version": "2.0.0",
    "description": "Enterprise proxy service for external Astro Engine API with scalability features",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc && tsc-alias",
        "start": "node dist/server.js",
        "test": "jest --passWithNoTests",
        "lint": "eslint src",
        "format": "prettier --write src"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "opossum": "^8.1.3",
        "pino": "^8.16.2",
        "prom-client": "^15.1.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.55.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "prettier": "^3.0.0",
        "ts-node-dev": "^2.0.0",
        "tsc-alias": "^1.8.8",
        "typescript": "^5.3.2"
    }
}